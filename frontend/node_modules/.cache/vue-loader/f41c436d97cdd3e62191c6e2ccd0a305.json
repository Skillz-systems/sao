=\"border-radius: 20px\">\r\n              <b-input-group-prepend\r\n                is-text\r\n                style=\"background-color: #F9F9F9\"\r\n              >\r\n                <feather-icon icon=\"SearchIcon\" />\r\n              </b-input-group-prepend>\r\n              <b-form-input\r\n                v-model=\"filter\"\r\n                placeholder=\"Quick Search\"\r\n              />\r\n            </b-input-group>\r\n          </b-col>\r\n          <b-col\r\n            lg=\"4\"\r\n            class=\"d-flex mt-lg-0 mt-1 text-center\"\r\n          >\r\n            <label style=\"padding: 10px 0\">Show</label>\r\n            <b-form-select\r\n              v-model=\"perPage\"\r\n              :options=\"perPageOptions\"\r\n              class=\"per-page-selector d-inline-block ml-50 mr-50\"\r\n            />\r\n            <label style=\"padding: 10px 0\">entries</label>\r\n          </b-col>\r\n        </b-row>\r\n      </b-col>\r\n    </b-row>\r\n    <b-row>\r\n      <b-col cols=\"12\">\r\n        <b-card\r\n          no-body\r\n          class=\"card-company-table\"\r\n        >\r\n          <b-table\r\n            ref=\"inventory-table\"\r\n            :items=\"getAuditTrail\"\r\n            responsive\r\n            hover\r\n            :fields=\"availableStockFields\"\r\n            :per-page=\"perPage\"\r\n            :current-page=\"currentPage\"\r\n            :filter=\"filter\"\r\n            show-empty\r\n            empty-text=\"Loading available stock...\"\r\n            class=\"mb-0\"\r\n          >\r\n            <template #cell(action)=\"data\">\r\n              <div class=\"text-nowrap\">\r\n                {{ data.item.action }}\r\n              </div>\r\n            </template>\r\n            <template #cell(created_at)=\"data\">\r\n              <div class=\"text-nowrap\">\r\n                {{ new Date(data.item.created_at).toUTCString() }}\r\n              </div>\r\n            </template>\r\n            <template #cell(user)=\"data\">\r\n              <p class=\"text-nowrap mb-0\">\r\n                {{ data.item.user.firstname }}\r\n              </p>\r\n              <small>\r\n                {{ data.item.user.email }}\r\n              </small>\r\n            </template>\r\n          </b-table>\r\n        </b-card>\r\n        <div class=\"mx-2 mb-2 mt-1\">\r\n          <b-row>\r\n            <b-col\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              class=\"d-flex align-items-center justify-content-center justify-content-sm-start\"\r\n            >\r\n              <span class=\"text-muted\">There are {{ totalRows }} entries</span>\r\n            </b-col>\r\n            <b-col\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              class=\"d-flex align-items-center justify-content-center justify-content-sm-end\"\r\n            >\r\n              <b-pagination\r\n                v-model=\"currentPage\"\r\n                :total-rows=\"totalRows\"\r\n                :per-page=\"perPage\"\r\n                class=\"mb-0 mt-1 mt-sm-0\"\r\n              />\r\n            </b-col>\r\n          </b-row>\r\n        </div>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      isLoading: false,\r\n      tableLoading: false,\r\n      perPage: 10,\r\n      perPageOptions: [5, 10, 20, 100],\r\n      filter: null,\r\n      availableStockFields: [\r\n        { key: 'action', label: 'Action' },\r\n        { key: 'ip', label: 'IP Address' },\r\n        { key: 'created_at', label: 'Logged' },\r\n        { key: 'user', label: 'User' },\r\n      ],\r\n      totalRows: 0,\r\n      currentPage: 1,\r\n    }\r\n  },\r\n  mounted() {\r\n    document.getElementById('custom-breadcrumb').innerHTML = 'Inventory / <span style=\"color: #C4C4C4\">Audit Log</span>'\r\n  },\r\n  methods: {\r\n    getAuditTrail(ctx, callback) {\r\n      this.$store.dispatch('inventory/getAuditTrail', { page: ctx.currentPage, perPage: ctx.perPage }).then(res => {\r\n        const { data, total } = res.data\r\n        console.log(res)\r\n        this.totalRows = total\r\n        callback(data)\r\n      }).catch(() => {\r\n        callback([])\r\n      })\r\n      return null\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n[dir] .input-group:not(.bootstrap-touchspin):focus-within .form-control, [dir] .input-group:not(.bootstrap-touchspin):focus-within .input-group-text {\r\n  border-color: #4D6D8A;\r\n}\r\n.per-page-selector {\r\n  width: 70px;\r\n}\r\n.invoice-filter-select {\r\n  min-width: 190px;\r\n  ::v-deep .vs__selected-options {\r\n    flex-wrap: nowrap;\r\n  }\r\n  ::v-deep .vs__selected {\r\n    width: 100px;\r\n  }\r\n}\r\n[dir] .custom-select:focus {\r\n  border-color: #4D6D8A;\r\n}\r\n.page-item.active .page-link {\r\n  background-color: #4D6D8A;\r\n  border-color: #4D6D8A;\r\n}\r\n.page-item .page-link:hover {\r\n  color: #4D6D8A;\r\n}\r\n[dir] .form-control:focus {\r\n  border-color: #4D6D8A;\r\n}\r\n[dir] .b-form-spinbutton.form-control:focus, [dir] .b-form-spinbutton.form-control.focus {\r\n  border-color: #4D6D8A;\r\n}\r\n\r\n.bartum-button {\r\n  color: white;\r\n  background-color: #4D6D8A !important;\r\n  border-color: #4D6D8A !important;\r\n}\r\n</style>\r\n\r\n<style lang=\"scss\">\r\n@import '@core/scss/vue/libs/vue-select.scss';\r\n</style>\r\n"]}]}